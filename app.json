{
  "expo": {
    "name": "Play Learn",
    "slug": "play-learn",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "playlearn",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.ayeshani.playlearn"
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", "expo-audio"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bec05981-4c7b-454d-82bb-25469125e533"
      }
    }
  }
}
