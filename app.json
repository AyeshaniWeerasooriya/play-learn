{
  "expo": {
    "name": "Play Learn",
    "slug": "play-learn",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "playlearn",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/frontIcon.png",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F7F9FC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.ayeshani.playlearn"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F7F9FC",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-audio",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bec05981-4c7b-454d-82bb-25469125e533"
      }
    }
  }
}
